    $.fn.imagesPicker = function(op) {
        this.each(function(i,e){
            var _pk = $.extend( {
                baseUrl: base_url,
                path: "VCMediaManager/ajax",
                src: src_upload,
                dir_icon: "shg_admin/images/folder-icon.png",
                src_relative: "/",
                abs: true,
                ckeditor: false,
                manual: false,
                onSelect: function(){}
            }, op);
            var current;
            if(!_pk.manual){
                var parent = e.parentNode, this_html = parent.innerHTML;
                var new_html = '<div class="input-group img-picker">'
                    +  this_html + '<span class="input-group-addon clicker" style="cursor:pointer"><span class="icon-picture"></span></span>'
                    +  '</div>';
                parent.innerHTML = new_html;
                current =$(parent).find(".img-picker");
                current = current.find(".clicker");
            }else current = $(e);
            current.click(function(){
                if(_pk.manual){ _curent = e; }
                else{
                    _curent = $(this.parentNode.parentNode).find("input").get(0);
                }
                var tabapi =  $('#srvUpload').val();
                $("#modal_img_picker").modal("show");
                $("#cke-image-opts").hide();
                loadImages(_cr_dir,_pk);
                $("#modal_img_picker").modal("show");
                $("#cke-image-opts").hide();

                if(tabapi ==1){
                    $("#upload-holder-ip").show();
                    $('#modal_body_img_picker').show();
                }

            });
        });
        $("#srvUpload").change(function(){
            var val = $('#srvUpload').val();
            if(val ==1){
                $("#upload-holder-ip").show();
                $('#modal_body_img_picker').show();

            }else{
                $("#upload-holder-ip").hide();
                $('#modal_body_img_picker').hide();
            }
        });
        return this;
    };

